   "source": [
    "\n",
    "# Générer un signal sinusoïdal\n",
    "t = np.linspace(0, 1, 1000, False)  # 1 second\n",
    "sign = np.sin(2*np.pi*10*t)  # Signal sinusoïdal de fréquence 10Hz\n",
    "\n",
    "# Transformation de Fourier\n",
    "fourier_transform = np.fft.fft(sign)\n",
    "\n",
    "# Spectre de fréquences\n",
    "magnitude = np.abs(fourier_transform) # La magnitude \n",
    "frequency = np.linspace(0, 1000, len(magnitude)) # Axe des fréquences\n",
    "\n",
    "\n",
    "plt.figure(figsize=(10,5))\n",
    "plt.plot(frequency, magnitude)\n",
    "plt.title(\"Spectre de fréquences\")\n",
    "plt.xlabel(\"Fréquence (Hz)\")\n",
    "plt.ylabel(\"Magnitude\")"
   ]
  },
